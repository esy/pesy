{
  "name": "pesy-preprocess",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "releasedBinaries": [
        "PesyPreprocessApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "pesy-preprocess.lib"
      ],
      "bin": "TestPesyPreprocess.re"
    },
    "library": {
      "name": "pesy-preprocess.lib",
      "namespace": "PesyPreprocess"
    },
    "executable": {
      "require": [
        "lwt",
        "lwt.unix",
        "pesy-preprocess.lib"
      ],
      "bin": {
        "PesyPreprocessApp.exe": "PesyPreprocessApp.re"
      },
      "preprocess": [
        "pps",
        "lwt_ppx"
      ]
    }
  },
  "scripts": {
    "test": "esy b dune runtest"
  },
  "dependencies": {
    "ocaml": "4.7.x",
    "@esy-ocaml/reason": "*",
    "@opam/dune": ">=1.6.0",
    "@opam/lwt": "4.1.0",
    "@opam/lwt_ppx": "1.2.1",
    "pesy": "*",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  },
  "resolutions": {
    "pesy": "<RESOLUTION_LINK>"
  }
}
