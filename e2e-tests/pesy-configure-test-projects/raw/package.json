{
  "name": "raw",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "releasedBinaries": [
        "RawApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "raw.lib"
      ],
      "bin": "TestRaw.re"
    },
    "library": {
      "name": "raw.lib",
      "namespace": "Raw"
    },
    "executable": {
      "bin": {
        "RawApp.exe": "RawApp.re"
      },
      "rawBuildConfig": [
        "(libraries lwt lwt.unix raw.lib)",
        "(preprocess (pps lwt_ppx))"
      ]
    },
    "share": {
      "namespace": "RawAssets",
      "name": "raw.assets",
      "rawBuildConfigFooter": [
        "(install (section share_root) (files (asset.txt as asset.txt)))"
      ]
    }
  },
  "scripts": {
    "test": "esy b dune runtest"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/dune": "*",
    "@opam/lwt": "4.1.0",
    "@opam/lwt_ppx": "1.2.1",
    "pesy": "*",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  },
  "resolutions": {
    "pesy": "esy/pesy#595f01ccc2ff251c1860063014bc6a6924a64a19"
  }
}
