{
  "name": "pesy-virtual",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "pesy build",
    "release": {
      "releasedBinaries": [
        "PesyVirtualApp.exe"
      ]
    }
  },
  "buildDirs": {
    "lib": {
      "require": [
        "pesy-virtual/foo"
      ]
    },
    "foo": {
      "virtualModules": [
        "VirtualFoo"
      ]
    },
    "bar": {
      "implements": [
        "pesy-virtual/foo"
      ]
    },
    "baz": {
      "implements": [
        "pesy-virtual/foo"
      ]
    },
    "executable-bar": {
      "require": [
        "pesy-virtual/lib",
        "pesy-virtual/bar"
      ],
      "bin": {
        "PesyVirtualAppBar.exe": "PesyVirtualApp.re"
      }
    },
    "executable-baz": {
      "require": [
        "pesy-virtual/lib",
        "pesy-virtual/baz"
      ],
      "bin": {
        "PesyVirtualAppBaz.exe": "PesyVirtualApp.re"
      }
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test-bar": "esy x PesyVirtualAppBar.exe",
    "test-baz": "esy x PesyVirtualAppBaz.exe",
    "test": "esy x PesyVirtualAppBar.exe"
  },
  "dependencies": {
    "ocaml": "4.7.x",
    "dune": "*",
    "@esy-ocaml/reason": "*",
    "ocaml": "^4.4.0",
    "pesy": "*",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  },
  "resolutions": {
    "pesy": "esy/pesy#ba6359f25621280a8105d2ffc99d75d849c0d95a"
  }
}
