(* -*- tuareg -*- *)

#warnings "-40";;

open Dune;;
open Import;;

(* Check that [of_alist_multi] groups elements in the right order *)
String.Map.of_list_multi
  [ "a", 1
  ; "b", 1
  ; "a", 2
  ; "a", 3
  ; "b", 2
  ]
|> String.Map.to_list;;
[%%expect{|
- : (string * int list) list = [("a", [1; 2; 3]); ("b", [1; 2])]
|}]
